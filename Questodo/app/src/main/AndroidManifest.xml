<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pecesteam.questodo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainQuestodo"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainTodoList"/>
        <activity android:name=".MainDoingList"/>
        <activity android:name=".MainNewProject"/>
        <activity android:name=".MainDoneList"/>
        <activity android:name=".MainProfile"/>
        <activity android:name=".MainAchivements"/>
    </application>

</manifest>
